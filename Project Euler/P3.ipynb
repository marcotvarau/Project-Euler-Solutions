   "source": [
    "def prime_factors(n):\n",
    "    factors = []\n",
    "    divisor = 2\n",
    "    while n > 1:\n",
    "        while n % divisor == 0:\n",
    "            factors.append(divisor)\n",
    "            n //= divisor\n",
    "        divisor += 1\n",
    "    return factors[-1]\n",
    "\n",
    "# Exemplo de uso da função\n",
    "number = 600851475143\n",
    "prime_factors(number)\n"
   ]
