   "source": [
    "import numpy as np\n",
    "def soma_dos_quadrados(n):\n",
    "    return np.sum([i**2 for i in range(0,n+1)])\n",
    "def quadrado_da_soma(n):\n",
    "    return np.sum(list(range(0,n+1)))**2\n",
    "\n",
    "def diferenca(n):\n",
    "    return quadrado_da_soma(n) - soma_dos_quadrados(n)"
   ]
